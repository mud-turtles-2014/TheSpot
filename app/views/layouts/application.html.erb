<!DOCTYPE html>
<html>
<head>
  <title>TheSpot</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav>
    <%= link_to "NYC Brunch", "/" , class: 'logo'%>
    <ul class="nav">
      <!--li class="active"><%= link_to "home", "/" %></li-->
      <li class="search"><!--%= render "search" %--></li>
      <% if session[:user_id] %>
        <li><%= link_to "logout", logout_path %></li>
      <% else %>
        <li><%= link_to "login", login_path %></li>
      <% end %>
    </ul>
  </nav>
<%= yield %>

</body>
</html>
